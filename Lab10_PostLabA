#include<stdio.h>
#include<stdlib.h>

char* strcpy(char* strDest, const char* strSrc)

{

unsigned i;

for (i=0; strSrc[i] != '\0'; ++i)

strDest[i] = strSrc[i];
 strDest[i]='\0';
  return strDest;

  }

int main(){

char src[] ="Test Program";
char dest[100];

printf("source string: %s\n",src);
printf("After copying sting: \n New string= %s\n" ,strcpy(dest,src));

return 0;
}
